{
  "name": "codexten/yii-module-currency",
  "type": "library",
  "description": "Locale module for yii 2.0 or higher",
  "require": {
    "kartik-v/yii2-widget-select2": "@dev"
  },
  "autoload": {
    "psr-4": {
      "codexten\\yii\\modules\\currency\\": "src"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "2.0.x-dev"
    },
    "sconfig-plugin": {
      "moduleLocale": "config/moduleLocale.php",
      "migrationNamespaces": "config/migrationNamespaces.php"
    }
  }
}
